{
  "include": ["lib", "playground/useparams.ts"],
  "compilerOptions": {
    "lib": ["dom", "esnext"],
    "target": "esnext",
    "module": "esnext",
    "sourceMap": false,
    "incremental": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
      "dev_useparams": ["./lib/index.d.ts"]
    }
  }
}
